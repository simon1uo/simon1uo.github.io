{
  "extends": "@vue/tsconfig/tsconfig.dom.json",
  "compilerOptions": {
    "composite": true,
    "tsBuildInfoFile": "./node_modules/.tmp/tsconfig.app.tsbuildinfo",
    "lib": [
      "ES2015",
      "DOM"
    ],
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "./src/*"
      ]
    },
    "resolveJsonModule": true,
    "emitDeclarationOnly": true,
    "noEmit": false
  },
  "include": [
    "vite-env.d.ts",
    "auto-imports.d.ts",
    "docs/**/*",
    "docs/**/*.vue",
    "docs/**/*.d.ts"
  ]
}
